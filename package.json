{
  "name": "chat-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "clean": "rm -rf .next && rm -rf node_modules",
    "ins": "pnpm install && prisma generate",
    "ins:clean": "pnpm clean && pnpm install && prisma generate",
    "dev:clean": "pnpm ins:clean && pnpm run dev",
    "build": "next build",
    "build:clean": "pnpm ins:clean && pnpm run build",
    "start": "next start",
    "serve": "pnpm build && pnpm start",
    "serve:clean": "pnpm build:clean && pnpm start",
    "lint": "next lint",
    "format": "prettier --write \"**/*.{ts,tsx,mjs,js,jsx,md,mdx,css,yaml,yml}\" && eslint --fix \"**/*.{ts,tsx,mjs,js,jsx,md,mdx,css,yaml,yml}\"",
    "check": "prettier --check \"**/*.{ts,tsx,mjs,js,jsx,md,mdx,css,yaml,yml}\"",
    "check:types": "tsc --noEmit && rm -rf tsconfig.tsbuildinfo",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^1.2.12",
    "@ai-sdk/google": "^1.2.19",
    "@ai-sdk/openai": "^1.3.22",
    "@ai-sdk/react": "^1.2.12",
    "@prisma/client": "^6.9.0",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@tanstack/react-query": "^5.80.7",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@uploadthing/react": "^7.3.1",
    "ai": "^4.3.16",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.18.1",
    "globals": "^16.2.0",
    "lucide-react": "^0.516.0",
    "next": "15.3.3",
    "next-auth": "5.0.0-beta.28",
    "next-themes": "^0.4.6",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.12",
    "prisma": "^6.9.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.58.1",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^15.6.1",
    "sonner": "^2.0.5",
    "tailwind-merge": "^3.3.1",
    "uploadthing": "^7.7.2",
    "uui": "^1.0.7",
    "uuid": "^11.1.0",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@eslint/compat": "^1.3.0",
    "@eslint/eslintrc": "^3",
    "@eslint/js": "^9.29.0",
    "@hookform/resolvers": "^5.1.1",
    "@next/eslint-plugin-next": "^15.3.3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "eslint": "^9",
    "eslint-config-next": "^15.3.3",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.3.4",
    "typescript": "^5"
  }
}
